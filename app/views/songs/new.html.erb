<h1>Create a Song</h1>

<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%# f.label :genre_name %>          
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  
  <%= f.fields_for :notes do |note| %>
  <%= note.label :content, 'Note' %>
  <%= note.text_field :content %><br>
  <% end %>
    
  <%= f.submit %>
<% end %>

